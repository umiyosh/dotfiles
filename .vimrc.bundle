" Buffer {{{
  " buftabs:バッファ管理
  Bundle 'fholgado/minibufexpl.vim'
" }}}


" Encording {{{
  Bundle 'banyan/recognize_charcode.vim'
" }}}

" Utility {{{
  Bundle 'tpope/vim-pathogen'
  " vimproc : vimから非同期実行。vimshelleで必要
  Bundle 'Shougo/vimproc.git'
  " vimfiler : vimのfiler
  Bundle 'Shougo/vimfiler.git'
  " vim-altercmd : Ex command拡張
  Bundle 'tyru/vim-altercmd'
  " vim Interface to Web API
  Bundle 'mattn/webapi-vim'
  " cecutil.vim : 他のpluginのためのutillity1
  Bundle 'cecutil'
  " utillity
  Bundle 'L9'
  " Buffer管理のLibrary
  Bundle 'thinca/vim-openbuf'
  " vimdoc 日本語
  Bundle 'yuroyoro/vimdoc_ja'
  " vim上のtwitter client
  Bundle 'TwitVim'
  " Lingrのclient
  Bundle 'tsukkee/lingr-vim'
  " vimからGit操作する
  Bundle 'tpope/vim-fugitive'
  " gitgutter
  Bundle 'airblade/vim-gitgutter'
  " ステータスラインをカッコよくする
  Bundle 'Lokaltog/vim-powerline'
  " 分割を維持してバッファ削除
  Bundle 'rgarver/Kwbd.vim'
  " 現在開いている以外のバッファを全部削除
  Bundle 'duff/vim-bufonly'
  " perl proveコマンド実行
  Bundle 'umiyosh/vim-prove'
  " perl proveコマンド実行
  Bundle 'Shougo/vinarise'
  " トップっぽいところをカレントディレクトリに
  " http://blog.remora.cx/2012/08/syntax-check-with-syntastic-and-rooter.html
  Bundle 'airblade/vim-rooter'
  " markdownのアウトライン表示
  Bundle 'vim-scripts/VOoM'
  " foldtextの見た目をよくする
  Bundle 'LeafCage/foldCC'
  " taskpaper.vim
  Bundle 'davidoc/taskpaper.vim'
  " repeat.vim
  Bundle 'tpope/vim-repeat'
  " ag.vim
  Bundle 'rking/ag.vim'
  " ag.vim
  Bundle 'vim-scripts/DirDiff.vim'
  " benchvimrc-vim
  Bundle 'mattn/benchvimrc-vim'
  " dash.vim
  Bundle 'rizzatti/dash.vim'
  "
" }}}

" ColorSchema{{{{
  " color schema 256
  Bundle 'desert256.vim'
  Bundle 'mrkn256.vim'
  Bundle 'tomasr/molokai'
  Bundle 'yuroyoro/yuroyoro256.vim'
" }}}

" Unite {{{{
  " unite.vim : - すべてを破壊し、すべてを繋げ - vim scriptで実装されたanythingプラグイン
  Bundle 'Shougo/unite.vim'
  Bundle 'tsukkee/unite-help'
  Bundle 'Shougo/unite-outline'
  Bundle 'basyura/unite-rails'
  Bundle 'thinca/vim-unite-history'
  Bundle 'tsukkee/unite-tag'
  Bundle 'choplin/unite-vim_hacks'
  Bundle 'tacroe/unite-mark'
  Bundle 'Shougo/neomru.vim'
  Bundle 'zoncoen/unite-autojump'
" }}}

Bundle 'gmarik/vundle'

filetype plugin indent on
