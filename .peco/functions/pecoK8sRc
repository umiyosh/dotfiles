#!/usr/bin/env zsh

kubectl get rc | selector --prompt "[$0]"| awk '{print $1}'|sort | uniq | \
  pecoAfterAction \
    "kubectl describe rc " \
    "kubectl get rc " \
    "kubectl get rc --output=json " \
    "kubectl get rc --output=yaml " \
    "%%STDIN%% cat" \
    "%%STDIN%% paaTovim" \
    "%%STDIN%% pecoPipe" \
    "%%STDIN%% paaPbCopy"
